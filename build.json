{
    "into": "public/build",
    "collect": {
        "icon": {
            "directory": "src/icon",
            "into": "img/icon"
        }
    },
    "combine": {
        "admin-js": {
            "basename": "admin",
            "extension": "js",
            "files": [
                "node_modules/jquery/dist/jquery.min.js",
                "include:src/js/jars/admin.json"
            ],
            "into": "js",
            "wrapper": {
                "open": "src/js/jars/admin/scope-open.js",
                "close": "src/js/jars/admin/scope-close.js"
            }
        },
        "admin-css": {
            "basename": "admin",
            "extension": "css",
            "files": [
                "include:src/css/jars/admin.json"
            ],
            "into": "css"
        }
    }
}